{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "shopping list",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.png --prune=true --out=release-builds",
    "package-win": "electron-packager . Shopping-list --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icon.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingList\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon.png --prune=true --out=release-builds"
  },
  "author": "Roman Cabalum",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "electron-packager": "^15.4.0",
    "electron": "^18.0.3"
  }
}
